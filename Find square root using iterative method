#include <iostream>
using namespace std;

int main() {
    double n;
    cout << "Enter number: ";
    cin >> n;

    double guess = n / 2;
    for(int i = 0; i < 10; i++) {   // 10 iterations
        guess = (guess + n / guess) / 2;
    }
    cout << "Approximate square root = " << guess;
    return 0;
}
