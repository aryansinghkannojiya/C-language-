#include <iostream>
using namespace std;

int gcd(int a, int b) {
    while(b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}

int main() {
    int a, b;
    cout << "Enter two numbers: ";
    cin >> a >> b;

    int lcm = (a * b) / gcd(a, b);

    cout << "LCM of " << a << " and " << b << " = " << lcm;
    return 0;
}
